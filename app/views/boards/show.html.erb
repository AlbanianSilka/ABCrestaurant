<p id="notice"><%= notice %></p>

<p>
  <strong>Board number:</strong>
  <%= @board.board_number %>
</p>
<p>
  <strong>Total seats: </strong>
    <%= @board.board_seats %>
</p>
<p>
  <strong>All bookings: </strong>
  <% @board.bookings.each do |x| %>
    <li><%= "Starts at #{x.start_time} ends at #{x.end_time}" %></li>
  <% end %>
</p>

<%= link_to 'Book a table', new_board_booking_path(@board) %>
<%= link_to 'Edit', edit_board_path(@board) %> |
<%= link_to 'Back', boards_path %>